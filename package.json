{
  "name": "x-post-reviewer",
  "version": "1.0.0",
  "description": "X post reviewer",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run dev:next & npm run dev:routes",
    "dev:next": "next dev -p 3002",
    "dev:routes": "next-typesafe-path --watch",
    "build": "npm run generate-icons && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "generate-icons": "node scripts/generate-icons.mjs"
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "1.13.10",
    "@google/generative-ai": "0.24.0",
    "@tailwindcss/postcss": "4.0.15",
    "date-fns": "4.1.0",
    "next": "15.1.7",
    "next-pwa": "5.6.0",
    "next-typesafe-path": "0.0.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.5.0",
    "tailwindcss": "4.0.15",
    "twitter-text": "3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/twitter-text": "3.1.10",
    "autoprefixer": "10.4.21",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "postcss": "8.5.3",
    "typescript": "5.8.2"
  }
}
